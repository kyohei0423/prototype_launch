var $answerOrReactions = $("#answer-or-reactions"),
    $answeredUserCount = $('#answered_user_count'),
    $answeredUserLists = $('#answered_users');

$answerOrReactions.html(
  "<%= j(render partial: 'answer_or_reaction_btn', locals: { question: @question } ) %>");
$answeredUserCount.attr('data-badge', "<%= @question.count_answered_users %>");
$answeredUserLists.html(
  "<%= j(render partial: 'questions/user', collection: @question.answered_users) %>"
);
